   "source": [
    "import pandas as pd\n",
    "import io\n",
    "\n",
    "city = r\"\"\"city,state\n",
    "sydney,nsw\n",
    "brisbane,qld\n",
    "perth,sa\n",
    "\"\"\"\n",
    "\n",
    "df = pd.read_csv(io.StringIO(city))\n",
    "\n",
    "df['state'] = df['state'].apply(lambda x: x.upper())\n",
    "df['city'] = df['city'].apply(lambda x: x.title())\n",
    "print(df)"
   ]
